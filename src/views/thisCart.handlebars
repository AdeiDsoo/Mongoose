<section class="container-InfoProductsCart">
<h2>Este es tu carrito </h2>
<p class="idCart">ID: {{infoThisCart.idCart}}</p>
<p class="idCart">{{infoThisCart.email}}</p>
<article class="infoProductsCart">
    <p>Tus productos son:</p>
    <table class="tableCart">
        <tr>
            <th>PRODUCT</th>
            <th>PRICE</th>
            <th>QUANTITY</th>
        </tr>
       {{#each infoThisCart.products}}
  <tr>
    <td>{{this.idProduct.title}}</td>
    <td>{{this.idProduct.price}}</td>
    <td>{{this.qty}}</td>
  </tr>
{{/each}}
    </table>
</article>
</section>
         